{
	"presets": [
		"react",
		[ "env", {
			"modules": false,
			"browser": true
		} ]
	],
	"env": {
		"test": {
			"presets": [ [ "env" ], "react" ]
		},
		"default": {
		  "plugins": [
			"lodash",
			"transform-runtime"
		  ]
		}
	},
	"plugins": [
	  	"transform-object-rest-spread",
		[ "transform-react-jsx", {
			"pragma": "wp.element.createElement"
		} ]
	]
}
